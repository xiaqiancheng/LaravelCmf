{
    "name": "xadmin/laravel-cmf",
    "description": "Laravel-cmf is a background management extension developed based on the laravel framework, which enables rapid build of back-end systems",
    "keywords": ["laravel", "admin", "panel"],
    "license": "MIT",
    "homepage": "https://github.com/xiaqiancheng/laravel-cmf/",
    "support": {
        "issues": "https://github.com/xiaqiancheng/laravel-cmf/issues",
        "source": "https://github.com/xiaqiancheng/laravel-cmf"
    },
    "authors": [
        {
            "name": "Xia QianCheng",
            "email": "qiancheng1019@foxmail.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "intervention/image": "^2.4",
        "doctrine/dbal": "^2.5",
        "larapack/doctrine-support": "~0.1.4",
        "arrilot/laravel-widgets": "^3.7",
        "league/flysystem": "~1.0.41",
        "composer/composer": "~1.4",
        "larapack/hooks": "~1.0.2",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpcov": "~4.0|~5.0|~6.0",
        "phpunit/phpunit": "~5.7.14|~6.1|~7.0|~8.0",
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "orchestra/testbench": "~3.0",
        "laravel/browser-kit-testing": "~2.0.0|~3.0.0|~4.0.0|~5.0.0|~5.1",
        "orchestra/testbench-browser-kit": "~3.5|~3.6|~3.7|~3.8"
    },
    "autoload": {
        "psr-4": {
            "XADMIN\\LaravelCmf\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XADMIN\\LaravelCmf\\Tests\\": "tests/"
      }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "XADMIN\\LaravelCmf\\LaravelCmfServiceProvider",
                "XADMIN\\LaravelCmf\\Providers\\LaravelCmfDummyServiceProvider",
                "XADMIN\\LaravelCmf\\LaravelCmfHooksServiceProvider"
            ]
        }
    }
}
